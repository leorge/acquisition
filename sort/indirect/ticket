#!/bin/sh
#
#   Compare Mergesort and Asymmetric Quicksort.
#
#   Usage:
#       $ performance $0 log2(N) [repeat]
#
#   Optional variables:
#       $size       : The size of an element
#       $threshold : Threshold N to switch algorithm to indirect sorting.
#
#   Command option for Sort:
#       -B n : Boundary number to change to an indirect sorting function
#       -q   : asymmetric Quicksort
#       -k   : ticKet_sort (extended tag sort)
#       -I m : indirect Mergesort
#       -I q : indirect Quicksort
#
# echo "Arguments : $*"
if [ $# -eq 0 ]; then
    echo "Usage : [size=<size>] [th=<threshold>] performance  $0  log2N  [repeat]"
else
    N=$1; shift
    boundary=${threshold:=$N}
    if ! Release/Sort -N $N -Z ${size:=16} -B $boundary -qkI qm $* ; then exit ; fi
fi

exit

# ticket16 in FX8300.ods

