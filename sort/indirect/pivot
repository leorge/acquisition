#!/bin/sh
LANG=C
if [ $# -lt 1 ]; then
    echo "usage : [size=xx]  [repeat=xx]  $0  log2(N)"
    exit
fi
log2N=$1; shift     # log2(N)
if [ $log2N -lt 20 ]; then log2N=20; fi

rm -f tmp[0-9]*

sed -i -e '1,/^@/!d' $0     # Delete data part
echo "start    : `date -u`" >> $0       # add date
echo "computer : `uname -norpv`" >> $0
echo "CPU      : `cpu_model`" >> $0
echo "operator : `id -un`" >> $0
echo "size     : ${size:=16}" >> $0
echo "repeat   : ${repeat:=1}" >> $0
echo "option   : $log2N" >> $0

#
#
# search boundary between middle and median of several elements
#
#   -q   : Asymmetric quicksort
#   -I q : Indirect Asymmetric quicksort
#   -I m : Indirect mergesort
#   -B n : Boundary number to change to an indirect sorting function
#   -P l,m,n,s,t : threshold to change the choice of Pivot.
#      N <= l -- middle element.
#      N <= m -- random element.
#      N <= n -- median of random 3 elements.
#      N <= s -- median of random 5 elements.
#      N <= t -- median of random (int(log2(N))-1)|1 elements.
#      else   -- median of random (int(log2(N))/2)|1 elements.
#

N=`echo 2^$log2N | bc`
for i in `seq 1 $repeat`; do echo "$0 `date` $i/$repeat" >&2
    random.awk $N > data
    for j in `seq 0 20`; do echo "$i:$j"; echo -n " $j" >&2
        k=`echo 2^$j | bc`
        Release/Sort -N $N -Z $size -I q  -B $N -P $k,1,$N,1    data | sed 's/iqsort(&)/median-of-3/'
        Release/Sort -N $N -Z $size -I q  -B $N -P $k,1,1,$N,1  data | sed 's/iqsort(&)/median-of-5/'
        Release/Sort -N $N -Z $size -I qm -B $N -P $k,1,1,1,1   data | sed 's/iqsort(&)/med-of-log/'
    done
    echo "" >&2
done > tmp$$
echo "stop     : `date -u`" >> $0       # add date

OUT=tmp.`basename $0`
et.awk -v 'x=log2(threshold)' tmp$$ > $OUT
expand -t `col.awk $OUT` $OUT | tee -a $0
echo "Raw data is in a file tmp$$"

exit

# pivot256 in phenom.ods

