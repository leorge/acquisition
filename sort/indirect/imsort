#!/bin/sh
#
#   Indirect mergesort
#
#   -q    : asymmetric Quicksort
#   -m    : top down Mergesort
#   -B $N : Boundary number to change algorithm from the simple asymmetric quicksort to an indirect sort.
#           $N skips the quicksort.
#   -I m  : Indirect Mergesort
#
# echo "Arguments : $*"
if [ $# -eq 0 ]; then
    echo "Usage : [size=<size>] performance  $0  log2N  [repeat]"
else
    N=$1; shift
    if ! Release/Sort -N $N -Z ${size:=16} -qm -B $N -I m $* ; then exit ; fi
fi

exit

# imsort16 in FX8300.ods

