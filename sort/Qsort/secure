#!/bin/sh
#
#   Compare quick_secure() to quick_hole().
#
#       -h : quicksort - prototype with Hole.
#       -S : quicksort - Secured quick_hole()
#
if [ $# -eq 0 ]; then
    echo "Usage : $0  Number  [data_file]"
else
    N=$1; shift
    Release/Sort -N $N -Z 16 -hS $*
fi

exit

