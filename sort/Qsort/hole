#!/bin/sh
#
#   Compare Dual Pivoot Quicksort and new Quicksort.
#
#       -f : quicksort - pivot is the First element with swaps.
#       -h : quicksort - prototype with Hole.
#
if [ $# -eq 0 ]; then
    echo "Usage : $0  Number  [data_file]"
else
    N=$1; shift
    Release/Sort -N $N -Z 16 -fh $*
fi

exit

