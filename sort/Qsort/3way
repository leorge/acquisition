#!/bin/sh
#
#   Compare 3-way Partitioning Quicksort and new Quicksort.
#       -h : quicksort - prototype with Hole.
#       -w : 3-way Partitioning Quicksort
#
if [ $# -eq 0 ]; then
    echo "Usage : $0  Number  [data_file]"
else
    N=$1; shift
    Release/Sort -N $N -whZ 16 $*
fi

exit

